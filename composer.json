{
  "name": "globegroup/notifier-bundle",
  "type": "symfony-bundle",
  "description": "Globe Group Notifier Bundle",
  "license": "proprietary",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Bartosz Piotr Pazola",
      "email": "bartosz.pazola@b2net.pl"
    }
  ],
  "require": {
    "php": "^7.3",
    "ext-curl": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "symfony/config": "*",
    "symfony/console": "*",
    "symfony/dependency-injection": "*",
    "symfony/dotenv": "*",
    "symfony/http-kernel": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "GlobeGroup\\NotifierBundle\\": ""
    }
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "^4.4|^5.0"
    },
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
